<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        logicalFilePath="221115.xml">

    <changeSet id="221115-1" author="alsturm">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="settings"/>
            </not>
        </preConditions>
        <createTable tableName="settings">
            <column name="id" type="BIGINT"><constraints nullable="false" primaryKey="true" primaryKeyName="settings_pkey"/></column>
            <column name="message" type="VARCHAR(1023)"/>
            <column name="delay" type="BIGINT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
